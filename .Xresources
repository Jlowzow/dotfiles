! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #53506b, #53506b, #53506b
rofi.color-normal: #53506b, #ffffff, #53506b, #53506b, #a883d2
rofi.color-active: #53506b, argb:ce268bd2, #53506b, #53506b, #fdf6e3
rofi.color-urgent: #53506b, #dc322f, #53506b, #dc322f, #53506b

